    "@saas-ui/charts": "^0.11.3",
    "@saas-ui/react": "^2.8.8",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.28.14",
    "@tanstack/react-router": "^1.58.15",
    "axios": "^1.7.7",
    "date-fns": "^3.6.0",
    "form-data": "4.0.0",
    "framer-motion": "^11.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.52.2",
