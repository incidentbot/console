    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-timeago": "^4.1.7",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "dotenv": "^16.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.13"
